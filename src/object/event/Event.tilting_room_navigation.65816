/**
* Tilting Room navigation prompts and wrong-door fallbacks.
* param1: navigation branch id; param2: fire/fall timeout flag.
*/
.include "src/object/event/abstract.Event.chapter_template_v3.h"

.section "Event.tilting_room_nav"

  METHOD init
  rep #$31
  CHAPTER_EVENT_INIT_PARAMS 2
  rts

  METHOD play
  rep #$31
  jsr Event.template.waitTimeline
  bcc +
